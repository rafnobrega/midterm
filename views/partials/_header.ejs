<header>
  <link rel="stylesheet" href="/styles/_header.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Rajdhani:wght@400;600&display=swap" rel="stylesheet">

  <nav class="navbar">
    <div class="nav-wrapper">
      <div id="nav-logo">
        <a class="nav-logo-button" href="/home"><img class="logo"
          src="https://i.ibb.co/zfrk9nC/Lotus-Flower.png"/></a>
      </div>
      <div class="navbar-items">
        <% if(!userId) { %>
        <a class="nav-item nav-link" href="/home">Home</a>
        <a class="nav-item nav-link" href="/menu">Menu</a>
        <a class="nav-item nav-link" href="/login">Login</a>
        <a class="nav-item nav-link" href="/register">Register</a>

        <% } if(userId){  %>
        <a class="nav-item nav-link" href="/home">Home</a>
       <a class="nav-item nav-link" href="/menu">Menu</a>
       <a class="nav-item nav-link" href="/cart">My Cart</a>
        <a class="nav-item nav-link" href="/orders">My Orders</a>
        <a class="nav-item nav-link" href="/logout">Logout</a>
          <% } if (userId === 3) { %>
            <a class="nav-item nav-link" href="/admin">Admin</a>
        <% }%>
      </div>
    </div>
  </nav>
</header>
